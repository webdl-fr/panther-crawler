{
    "name": "webdl/panther-crawler",
    "description": "Web crawler based on Panther",
    "keywords": ["scraping"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/event-dispatcher": "^5.3 || ^6.0",
        "symfony/panther": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webdl\\PantherCrawler\\": "src/"
        }
    },
    "authors": [
        {
            "name": "kissifrot",
            "email": "kissifrot+gh@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.7"
    }
}
